{
  "name": "assignment-bike-data",
  "version": "1.0.0",
  "description": "Assignment for solita dev academy",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --detectOpenHandles --coverage --verbose false",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "keywords": [
    "typescript",
    "mysql",
    "react"
  ],
  "author": "Nhan Nguyen",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.15",
    "csv-parser": "^3.0.0",
    "express": "^4.18.2",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20"
  }
}
